{
  "name": "web-image-categorizer",
  "version": "0.3.1",
  "description": "A browser extension to save webpage images to cloud storage using customizable naming templates.",
  "keywords": [
    "browser-extension",
    "firefox",
    "chrome",
    "webextension",
    "cloud-storage",
    "filelu",
    "s3",
    "aws-s3",
    "image-downloader",
    "image-saver",
    "naming-templates",
    "react",
    "typescript"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "author": "Albert L <hkalbertl@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@smithy/protocol-http": "^5.2.1",
    "@smithy/signature-v4": "^5.2.1",
    "@wxt-dev/webextension-polyfill": "^1.0.0",
    "bootstrap": "^5.3.8",
    "dayjs": "^1.11.18",
    "fast-xml-parser": "^5.2.5",
    "i18next": "^25.5.2",
    "matcher": "^6.0.0",
    "mime": "^4.1.0",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "wcipher": "^0.0.5",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@wxt-dev/module-react": "^1.1.3",
    "sass-embedded": "^1.92.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.11"
  }
}
